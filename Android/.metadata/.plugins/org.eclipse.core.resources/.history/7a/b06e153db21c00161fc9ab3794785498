import java.util.Scanner;

public class Actividad03_04 {
	public static long serieFibonaci(long n) {
		if (n == 0 || n == 1) {
			return n;
		}
		else {
			return serieFibonaci(n - 1) + serieFibonaci(n - 2);
		}
	}
	
	public static void main(String[] args) {
		Scanner teclado = new Scanner(System.in);
		int numero;
		
		System.out.println("Digite un numero para generar la serie: "); numero = teclado.nextInt();
		for ( int i = 0; i <= numero; i++ ) {
			System.out.println("Fibonaci de "+ i +" es = " + serieFibonaci(i));
		}

		teclado.close();
	}
}
