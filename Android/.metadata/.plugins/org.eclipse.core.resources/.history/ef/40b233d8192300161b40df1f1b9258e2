package Actividad05_06;

import java.util.HashSet;
import java.util.Iterator;


public class ActividadConjuntos {
	public static void main(String[] args) {
        Articulo a1 = new Articulo("001", "Sueter Polo", 50000);
        Articulo a2 = new Articulo("002", "Gorra Nike", 20000);
        Articulo a3 = new Articulo("003", "Zapatillas Adidas", 40000);
        Articulo a4 = new Articulo("004", "Bermuda Tommy", 3000);

        HashSet<Articulo> articulos = new HashSet<Articulo>();
        articulos.add(a1);
        articulos.add(a2);
        articulos.add(a3);
        articulos.add(a4);

        for(Iterator<Articulo> it = articulos.iterator(); it.hasNext();) { 
        	Articulo e = it.next();
	    	System.out.println(e.getNombre()+"-"+e.getPrecio());
        }		
	}
}
