package Actividad03_10;

import Actividad03_04.Producto;

import java.io.PrintStream;
import java.util.Scanner;;

public class PruebaProducto {
	public static void main(String[] args) {
		Producto producto = new Producto();
		Scanner teclado = new Scanner(System.in);
		PrintStream consola = System.out;
		
		consola.println("Datos Producto");
		consola.println("Codigo:"); producto.setCodigo(teclado.nextInt());
		consola.println("Nombre:"); producto.setCodigo(teclado.nextInt());
		consola.println("Descripcion:"); producto.setCodigo(teclado.nextInt());
		consola.println("Valor:"); producto.setCodigo(teclado.nextInt());
		consola.println("Cantidad:"); producto.setCodigo(teclado.nextInt());
	}
}
